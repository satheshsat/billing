{
  "name": "Billing",
  "version": "0.0.1",
  "homepage_url": "https://www.sathesh.web.app",
  "description": "Billing Application",
  "manifest_version": 2,
  "background": {
   "scripts": [
    "assets/js/background.js"
   ],
   "persistent": false
  },
  "browser_action": {
   "default_title": "Billing"
  },
  "permissions": [
   "notifications",
   "storage",
   "unlimitedStorage"
  ],
  "offline_enabled": true,
  "web_accessible_resources": [
      "assets/img/*",
      "assets/icon/*.png",
      "index.html"
    ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://connect.facebook.net https://www.googletagmanager.com https://cdn.firebase.com https://*.firebaseio.com; object-src 'self'"
 }